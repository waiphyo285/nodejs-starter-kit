div.modal.fade#dialogLogoutConfirm(tabindex='-1', role='dialog', aria-labelledby='logoutConfirm')
  div.modal-dialog.modal-dialog-centered.modal-md(role='document')
    div.modal-content
      div.modal-header
        h5.modal-title 
          | !{content.modal.warning}
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') &times;
      div.modal-body
        h1.text-center
          i.bi.bi-box-arrow-right.text-warning
        p.text-center !{content['modal-lbl'].logout_q}
      div.modal-footer.justify-content-md-center
        button.btn.material-button.material-secondary#dialogCancel(type='button', data-dismiss='modal')
          |  !{content.common.close}
        button.btn.material-button.material-primary#dialogLogout(type='button', data-dismiss='modal')
          i.bi.bi-box-arrow-right
          |  !{content.navbar.logout}