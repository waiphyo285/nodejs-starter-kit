script.
  // Global Variable
  var table, pageEntry;
  var token = '!{user.latmat}';
  var userId = '!{user._id}';
  var role = '!{user.role}';
  var signJwtMethod = "one";
  var maxImageCount = 5;

  // request permission to user but not still use
  //- if (Notification.permission != "granted") {
  //-   Notification.requestPermission(); 
  //- }

  function dataTableDef (curPage) { 
    const buttons = [
      'excel',
      'pdf', 
      'print'
    ];

    const options = {
      exportOptions: {
        modifier: {
          order: 'index',
          page: 'all',
          search: 'none'
        },
        columns: ':visible'
      }
    };

    const langOptions = {
      "searchPlaceholder": "Search",
      "paginate": {
        "previous": "<",
        "next": ">"
      }
    };

    const buttonOptions  = buttons.map((button) => { 
      return {
        filename: curPage,
        extend: button,
        ...options
      }
    });

    return {
      "destroy": true,
      "scrollX": true,
      "autoWidth": true,
      "serverSide": true,
      //- "processing": true,
      "pagingType": "full_numbers",
      "language": langOptions,
      "buttons": buttonOptions
    };
  }

  function fetchDataDef (curUrl) {
    return {
      "url": "./api" + curUrl,
      "headers": {
        "x-access-method": signJwtMethod,
        "authorization": "Bearer " + token
      },
      "dataSrc": "data",
    };
  }

